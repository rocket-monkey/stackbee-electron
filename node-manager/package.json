{
  "name": "hgo-manager",
  "version": "0.0.1",
  "description": "Scripts for the managing server of stackbee.io",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "babel-node app.js",
    "start-debug": "yarn run build && node --inspect app.js",
    "build": "rimraf dist && babel -d dist/ src/",
    "sbm-install": "npm install -g",
    "sbm": "./index.js",
    "oc": "babel-node ./bin/owncloud/index.js",
    "oc-create": "babel-node ./bin/owncloud/ecsCreate.js",
    "oc-kill": "babel-node ./bin/owncloud/ecsKill.js",
    "oc-new-user": "babel-node ./bin/owncloud/newUser.js",
    "docker-build": "yarn run build && docker build -t stackbeeio/node-manager .",
    "docker-push": "docker push stackbeeio/node-manager",
    "postinstall": "npm run build"
  },
  "bin": {
    "sbm": "./index.js"
  },
  "repository": "https://github.com/rocket-monkey/stackbee.io",
  "keywords": [
    "cli",
    "node.js"
  ],
  "author": "Remo Vetere",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/rocket-monkey/stackbee.io/issues"
  },
  "homepage": "https://github.com/rocket-monkey/stackbee.io#readme",
  "dependencies": {
    "amazonops": "^0.0.2",
    "aws-sdk": "^2.84.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2017": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.18.2",
    "colors": "^1.1.2",
    "csv-parse": "^2.0.0",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.1",
    "mongoose": "^4.10.5",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "multer-s3": "^2.7.0",
    "node-gyp": "^3.6.2",
    "node-table-to-csv": "^0.1.0",
    "object-hash": "^1.1.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "rimraf": "^2.6.2"
  }
}
