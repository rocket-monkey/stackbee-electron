{
  "name": "hgo-manager",
  "version": "0.0.1",
  "description": "Scripts for the managing server of stackbee.io",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "babel-node app.js",
    "compile": "babel -d lib/ src/",
    "sbm-install": "npm install -g",
    "sbm": "./index.js",
    "oc": "babel-node ./bin/owncloud/index.js",
    "oc-create": "babel-node ./bin/owncloud/ecsCreate.js",
    "oc-kill": "babel-node ./bin/owncloud/ecsKill.js",
    "oc-new-user": "babel-node ./bin/owncloud/newUser.js",
    "docker-build": "yarn run compile && docker build -t stackbeeio/node-manager .",
    "docker-push": "docker push stackbeeio/node-manager",
    "postinstall": "npm run compile"
  },
  "bin": {
    "sbm": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocket-monkey/hgo-manager.git"
  },
  "keywords": [
    "cli",
    "node.js"
  ],
  "author": "Remo Vetere",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/rocket-monkey/hgo-manager/issues"
  },
  "homepage": "https://github.com/rocket-monkey/hgo-manager#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "colors": "^1.1.2",
    "express": "^4.15.3",
    "mongoose": "^4.10.5"
  }
}
